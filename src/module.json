      "path": "lang/es.json"
    },
    {
      "lang": "ko",
      "name": "Korean",
      "path": "lang/ko.json"
    },
    {
      "lang": "ja",
      "name": "日本語",
      "path": "lang/ja.json"
    }
  ],
  "url": "https://github.com/bmarian/token-tooltip-alt",
  "manifest": "https://raw.githubusercontent.com/bmarian/token-tooltip-alt/master/src/module.json",
  "download": "https://github.com/bmarian/token-tooltip-alt/releases/download/v2.1.3/token-tooltip-alt-v2.1.3.zip",
  "changelog": "https://github.com/bmarian/token-tooltip-alt/releases"
}