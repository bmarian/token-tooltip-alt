{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-undef": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "prefer-rest-params": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": "off"
    }
}
